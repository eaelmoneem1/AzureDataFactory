{
	"name": "FileShare3",
	"properties": {
		"linkedServiceName": {
			"referenceName": "SharedFolder3",
			"type": "LinkedServiceReference"
		},
		"type": "FileShare",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@concat ('FinanceActuals_' , formatDateTime(utcnow(), 'yyyy'), formatDateTime(utcnow(), 'MM'), formatDateTime(utcnow(), 'dd'),'.csv')",
				"type": "Expression"
			},
			"folderPath": "Storage"
		}
	}
}