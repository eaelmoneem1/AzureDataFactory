{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "p21d19901010001"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/EXMSCORE_Age_Group_Source')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "FileShare",
					"type": "LinkedServiceReference"
				},
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "FileServerLocation",
						"fileName": {
							"value": "@concat ('EXM_AgeGroup_' , formatDateTime(utcnow(), 'yyyy'), formatDateTime(utcnow(), 'MM'), formatDateTime(utcnow(), 'dd'),'.csv')",
							"type": "Expression"
						},
						"folderPath": "CEOCFO/HR"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Age Group",
						"type": "String"
					},
					{
						"name": "Responses",
						"type": "String"
					},
					{
						"name": "Detractors",
						"type": "String"
					},
					{
						"name": "Passives",
						"type": "String"
					},
					{
						"name": "Promoters",
						"type": "String"
					},
					{
						"name": "Score",
						"type": "String"
					},
					{
						"name": "Avg. Response",
						"type": "String"
					},
					{
						"name": "Response Distribution 0",
						"type": "String"
					},
					{
						"name": "Response Distribution 1",
						"type": "String"
					},
					{
						"name": "Response Distribution 2",
						"type": "String"
					},
					{
						"name": "Response Distribution 3",
						"type": "String"
					},
					{
						"name": "Response Distribution 4",
						"type": "String"
					},
					{
						"name": "Response Distribution 5",
						"type": "String"
					},
					{
						"name": "Response Distribution 6",
						"type": "String"
					},
					{
						"name": "Response Distribution 7",
						"type": "String"
					},
					{
						"name": "Response Distribution 8",
						"type": "String"
					},
					{
						"name": "Response Distribution 9",
						"type": "String"
					},
					{
						"name": "Response Distribution 10",
						"type": "String"
					},
					{
						"name": "Year",
						"type": "String"
					},
					{
						"name": "Week",
						"type": "String"
					}
				]
			}
		}
	]
}